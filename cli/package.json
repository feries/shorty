{
  "name": "cli",
  "version": "0.0.0",
  "author": "Feries s.r.l <info@feries.it> (https://www.feries.it)",
  "contributors": [
    "Claudio Quaglia <claudio.quaglia@feries.it>",
    "Gabriele Pellegrini <gabriele.pellegrini@feries.it>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/feries/shorty.git"
  },
  "bugs": "https://github.com/feries/shorty/issues",
  "engines": {
    "node": ">= 10.15.0",
    "yarn": ">= 1.10.0"
  },
  "bin": {
    "bin": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^2.4.2",
    "cli-ux": "^5.2.1",
    "dotenv": "^7.0.0",
    "got": "^9.6.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "bin",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
